TIPO -> int|string|float|char|bool|int\[\]|string\[\]|float\[\]|char\[\]|bool\[\]|int\[\]\[\]|string\[\]\[\]|float\[\]\[\]|char\[\]\[\]|bool\[\]\[\]

IF -> if ( {CONDICAO} ) begin {BODY} end | if ( {CONDICAO} ) begin {BODY} end else {BODY} end

FOR -> for ( int id = number to number ) begin {BODY} end

BODY -> 

CONDICAO -> 

